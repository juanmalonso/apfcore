<div class="ui field" v-bind:class="{required: getUiOption('required')}">

    <select class="ui search dropdown" v-bind:class="{disabled: getUiOption('readOnly')}" ref="realField" v-bind:name="field.id + '[]'" multiple="" v-bind:form="forForm"></select>

    <p v-if="getUiOption('info') != ''" style="font-size:.8em">${ getUiOption('info') }</p>
</div>
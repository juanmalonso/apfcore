{
  "accessControl": true,
  "title": "Tablero Principal",
  "navigation": {
    "user": {
      "label": "v{session(user_login)}",
      "url": "http://localhost/profile",
      "avatar": "f{getSession(user_avatar)}"
    },
    "items": [
      {
        "label": "Inicio",
        "url": "http://localhost/board/",
        "icon": "home icon"
      }
    ]
  },
  "application": {
    "type": "dataobjectscrud",
    "serviceActions":{
      "default":"list",
      "paramNum":5
    },
    "dataSources": {
      "objects":{
        "adapter":"objects",
        "options":{
          "model":"affiliates"
        }
      }
    },
    "topbar":{
      "list":{
        "title":{
          "text":"MAIN BOARD"
        },
        "actions":[
          {
            "label":"nuevo",
            "urlMap":"#",
            "icon":"plus green icon"
          }
        ]
      }
    },
    "selector": {
      "type": "tablelist",
      "dataSource":"objects"
    },
    "urlMaps":{
      "LIST":"v{service('baseurl')}/board/list",
      "EDIT":"v{service('baseurl')}/board/edit/{ROW:_id}",
      "DELETE":"v{service('baseurl')}/board/delete/{ROW:_id}"
    }
  }
}

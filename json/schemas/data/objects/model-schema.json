{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type":"object",
    "properties": {
        "id": {
            "type": "string",
            "maxLength": 50,
            "pattern": "^[A-Z1-9]{3,20}$"
        },
        "type": {
            "type": "string",
            "enum": [
                "OBJECT",
                "RELATION",
                "COLLECTION"
            ]
        },
        "idStrategy":{
            "type":"string",
            "enum": [
                "UUID",
                "SLUG",
                "CUSTOM",
                "SLUGPREFIX",
                "AUTOINCREMENT",
                "BASE36"
            ]
        },
        "partitionMode":{
            "type":"string",
            "enum": [
                "NONE",
                "Y4",
                "Y12",
                "Y53",
                "Y122"
            ]
        }
    }
}